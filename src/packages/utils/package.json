{
  "name": "@excalidraw/utils",
  "version": "0.1.2",
  "main": "dist/excalidraw-utils.min.js",
  "files": [
    "dist/*"
  ],
  "description": "Excalidraw utilities functions",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "keywords": [
    "excalidraw",
    "excalidraw-utils"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all",
      "not safari < 12",
      "not kaios <= 2.5",
      "not edge < 79",
      "not chrome < 70",
      "not and_uc < 13",
      "not samsung < 10"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "file-loader": "6.2.0",
    "webpack": "5.72.1",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.2"
  },
  "bugs": "https://github.com/excalidraw/excalidraw/issues",
  "repository": "https://github.com/excalidraw/excalidraw",
  "scripts": {
    "build:umd": "cross-env NODE_ENV=production webpack --config webpack.prod.config.js",
    "build:umd:withAnalyzer": "cross-env NODE_ENV=production ANALYZER=true webpack --config webpack.prod.config.js",
    "install:deps": "yarn --cwd ../ install --frozen-lockfile && yarn install --frozen-lockfile && yarn --cwd ../../../ install --frozen-lockfile",
    "pack": "yarn build:umd && yarn pack"
  }
}
